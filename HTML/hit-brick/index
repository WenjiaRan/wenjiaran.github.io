<script>
 
function showMsgNum(){
	localStorage.removeItem("isShowMsgNum7");
	var isShowMsgNum = localStorage.getItem("isShowMsgNum8");
	if(isShowMsgNum){
				$("#msgnum").hide();
	}
}
function todaygame(url){
	var isShowMsgNum = localStorage.getItem("isShowMsgNum8");
	if(isShowMsgNum){
				location.href="/dayindex";
	}
	else{
		localStorage.setItem("isShowMsgNum8",true);
		location.href=url;
	}
	

	
}
function showAppdl(){
	var indeurl = window.location.href;
	if(indeurl.indexOf('t=1')>-1){
		localStorage.setItem("isShowAppdl",true);
	}
	var isShowAppdl = localStorage.getItem("isShowAppdl");
	var u = navigator.userAgent;
	if(!isShowAppdl&&u.indexOf('iPhone')==-1){
		$("#msgnumdl").show();
		$("#appdl").show();
	}
	
}
function showads1(){
	var isShowAdsl = localStorage.getItem("isShowAdsl");
	var now = new Date();
    var month = now.getMonth();
	var date = now.getDate();
	if(isShowAdsl!=month+date){
		$("#msgnumdl").show();
	}
}
function openads1(){
	var isShowAdsl = localStorage.getItem("isShowAdsl");
	var now = new Date();
    var month = now.getMonth();
	var date = now.getDate();
	if(isShowAdsl==month+date){
		alert("感谢支持，今天已经支持过了");
	}
	else{
		alert("感谢支持，即将在新页面打开广告页面。");
		localStorage.setItem("isShowAdsl",month+date);
		var newWin = window.open();
		newWin.location.href = "https://oyo.lpjxdt.cn:10443/oyo/zl?uid=681&zid=955";
	}
}
function downloadapp(){
			location.href='/app/yikm_1.apk';
}
 
$(document).ready(function(){
                        var date = new Date();
		var mon = date .getMonth(); 
		var day = date .getDate();
		var extime = mon+":"+day;
		var lextime = localStorage.getItem("adsexptime");
		var v = "";
		if(v=='1'){localStorage.setItem("adsexptime", extime);localStorage.setItem("v2", "2");}
		if(extime !=lextime){
			localStorage.setItem("adsexptime", extime);} 
			//showads1();
			showMsgNum();
			//showAppdl();
			//$('#ads1').attr('href','https://oyo.lpjxdt.cn:10443/oyo/zl?uid=681&zid=955');
			$('.img').css("height",$('.img:first').width());
			$('.card').css("height",$('.card:first').height());
			var tag ='';
			if(tag){
				$("#menuul li").removeClass("active");
				$("#menuul li").eq(parseInt(tag)).addClass('active');
			}
			$("#back-top").click(function(){
				 $("html").animate({"scrollTop": "0px"},100);
				 $("body").animate({"scrollTop": "0px"},100);
			});
			
			/*var isshowUpdate = localStorage.getItem("isShowUpdate1");
			if(!isshowUpdate){
				$("#controllerContent").show();
			}*/

$("#closecontroller").click(function(){
				 $("#controllerContent").hide();
				 localStorage.setItem("isShowUpdate1",true);
			});
			var u = navigator.userAgent;
			var isshow = localStorage.getItem("isShowAddDesktop");
			if(u.indexOf('iPhone')>-1 && u.indexOf('Safari')>-1&&!isshow){
				$(".showAddTip").show();
				localStorage.setItem("isShowAddDesktop",true);
			}
			$(".add2desktop-close").click(function(){
				$(".add2desktop").hide();
				localStorage.setItem("isShowAddDesktop",true);
			});
			var appversion = navigator.appVersion;
			if(u.indexOf('iPhone')>-1 && (appversion.split('UCBrowser/').length > 1||appversion.indexOf("Quark")>-1) ){
				$(".changeUCTip").show();
			}
			var etype = '',tag='';
			if(u.indexOf('iPhone')==-1&&("1"==etype||"9"==tag)&& (appversion.split('UCBrowser/').length > 1||appversion.indexOf("Quark")>-1) ){
				$(".changeUCTip2").show();
			}
			$(window).scroll(function(){
				if(checkismobile()){
				        if($(window).scrollTop() >= 100){
				            $(".navbar").removeClass('navbar-transparent').addClass('filled');
							$("#msgnum").css("top","22px");
							$("#msgnumdl").css("top","22px");
				        } else{
				        	$(".navbar").removeClass('filled').addClass('navbar-transparent');
														$("#msgnum").css("top","42px");
														$("#msgnumdl").css("top","42px");

				        }}
						
				        if($(window).scrollTop() >= 800){
				            $("#back-top").show();
				        } else{
				        	$("#back-top").hide();
				        }
				    });
			var gameHistory = window.localStorage.gameHistory;
			var indeurl = window.location.href;
			if(gameHistory){
				gameHistory = JSON.parse(gameHistory);
				for(var i=0;i<gameHistory.length;i++){
					var li = '<li class="hot-li tm_list-img"><a href="'+gameHistory[i].url+'"><img class="hot-li tm_list-img" src="'+gameHistory[i].pic.replaceAll("https://mrcuya.gitee.io/yikmresource","https://img.1990i.com")+'"></img>'+gameHistory[i].name+'</a></li>';
					$("#game-history-ul").append(li);
				}
				
			}
			else{
				$("#game-history").css("margin-bottom","40px");
			}
			
			if(!checkismobile()&&indeurl.indexOf("search")!=-1){
			
				$(".navbar-search-form").css({"float":"right","margin-top":"30px","right":"calc(50% - 70px)","position":"relative"});
				/*$(".topright").css("margin-top","20px");
				$(".al2").each(function(index, value) {
						if(index==0){
							$(value).show();
						}
				});*/
			}
			/*if(checkismobile()&&indeurl.indexOf("search")!=-1){
			
				
				$(".al2").each(function(index, value) {
						if(index!=0){
							$(value).show();
						}
				});
			}*/
			/*if(indeurl.indexOf("search")!=-1){
				if(!checkismobile()){
					$("#game-history").css("margin-top","150px");
				}
				else
				$("#game-history").css("margin-top","90px");
			}*/
			if(indeurl.indexOf("tag")==-1&&indeurl.indexOf("search")==-1)
				$("#game-history").show();
			
				
	　　}); 


</script>

